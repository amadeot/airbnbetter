<%= form_for @listing do |f| %>
	<%= f.label :address %>
	<%= f.text_field :address %>
	<%= f.label :neighborhood %>
	<%= f.text_field :neighborhood %>
	<%= f.label :price %>
	<%= f.number_field :price %>
	<%= f.label :img_url %>
	<%= f.text_field :img_url %>
	<%= f.label :description %>
	<%= f.text_area(:description, size: '50x10') %>
	<%= f.submit %>
<% end %>